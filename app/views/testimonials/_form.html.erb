<%= simple_form_for(@testimonial) do |f| %>
  <%= f.error_notification %>

  <div class="form-group">
    <%= f.input :star_rating %>
  </div>
  <div class="form-group">
    <%= f.input :body, label: 'Your Review', input_html: {class: 'form-control', rows:5} %>
  </div>
   
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
